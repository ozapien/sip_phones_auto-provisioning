forward.no_answer.timeout = 3
forward.no_answer.on_code = {{ FREEPBX_CF_NANSWR_ACTIVATE_CODE }}
forward.no_answer.off_code = {{ FREEPBX_CF_NANSWR_DEACTIVATE_CODE }}